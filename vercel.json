{
  "version": 2,
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/test-env",
      "dest": "/api/test-env.js"
    },
    {
      "src": "/send-upload-link",
      "dest": "/api/send-upload-link.js"
    },
    {
      "src": "/api/send-upload-link",
      "dest": "/api/send-upload-link.js"
    },
    {
      "src": "/api/update-profile",
      "dest": "/api/update-profile.js"
    },
    {
      "src": "/api/profiles",
      "dest": "/api/profiles.js"
    },
    {
      "src": "/profile/(.*)",
      "dest": "/public/profile.html"
    },
    {
      "src": "/status/(.*)",
      "dest": "/api/status.js"
    },
    {
      "src": "/api/status",
      "dest": "/api/status.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
